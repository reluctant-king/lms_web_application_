{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "src": "^/favicon.ico$",
      "dest": "/pngegg.png"
    },
    {
      "src": "^/(?!assets/|_app/|_static/|favicon.ico|robots.txt|sitemap.xml).*$",
      "dest": "/index.html"
    }
  ]
}
